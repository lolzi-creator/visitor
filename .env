# Token Configuration
TOKEN_IDENTIFIER=distribute

# Request Configuration
REQUEST_INTERVAL=30000
CONCURRENT_REQUESTS=2

# Browser Behavior
SCROLL_PAGE=true
INTERACT_WITH_PAGE=true

# Logging
LOG_LEVEL=info

# CoinGecko Support (for future use)
COINGECKO_ENABLED=false

# Visit Configuration
TOTAL_VISITS=10         # Total number of visits to make
CONCURRENT_SESSIONS=4   # Number of parallel sessions
VISIT_DELAY=5000       # Delay between batches in milliseconds (15 seconds)

# Browser Behavior (true or false)
HEADLESS=true           # Run browsers in headless mode